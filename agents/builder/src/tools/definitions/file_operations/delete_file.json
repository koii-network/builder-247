{
  "name": "delete_file",
  "description": "Delete a file and commit the change.",
  "input_schema": {
    "type": "object",
    "properties": {
      "file_path": {
        "type": "string",
        "description": "Path to the file to delete"
      },
      "commit_message": {
        "type": "string",
        "description": "Commit message for the change"
      }
    },
    "required": ["file_path", "commit_message"]
  }
}
