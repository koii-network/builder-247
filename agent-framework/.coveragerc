[run]
source = prometheus_swarm
omit = 
    */tests/*
    */__init__.py
    */__main__.py
    */clients/base_client.py
    */old/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass

[html]
directory = coverage_html_report